{
  "name": "teckos-example",
  "version": "0.0.1",
  "description": "Example using the teckos server library",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "DEBUG=* nodemon --watch src -e ts --exec 'ts-node' --project tsconfig.json src/index.ts",
    "build": "rimraf dist && tsc --p tsconfig.json",
    "lint": "npx eslint ./src --ext .ts --resolve-plugins-relative-to .",
    "lint:fix": "npx eslint ./src --ext .ts --resolve-plugins-relative-to . --fix",
    "start": "npm run build && node ./dist/index.js"
  },
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0",
    "teckos": "file:.."
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
