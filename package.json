{
  "name": "teckos",
  "version": "0.8.1",
  "description": "uWebsocket.js based server component supporting async callbacks",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./uws": {
      "import": "./uws/index.mjs",
      "require": "./uws/index.cjs"
    }
  },
  "types": "./types/index.d.ts",
  "files": [
    "types",
    "src",
    "lib",
    "uws",
    "npm-scripts.mjs"
  ],
  "sideEffects": false,
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "postinstall": "node npm-scripts.mjs",
    "ts-types": "tsc -p tsconfig.json",
    "es-check": "es-check es2020 lib/index.mjs --module",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register '__tests__/**/*.ts'",
    "build": "rimraf lib types && node esbuild.config.cjs && npm run ts-types && npm run es-check",
    "lint": "npx eslint ./src --ext .js,.ts",
    "preversion": "npm run lint && npm run build && npm run es-check && rimraf uws/*.node",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delude88/teckos.git"
  },
  "keywords": [
    "websocket",
    "uwebsocket",
    "realtime",
    "callbacks"
  ],
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delude88/teckos/issues"
  },
  "homepage": "https://github.com/delude88/teckos#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/ioredis": "^4.28.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "es-check": "^6.2.1",
    "esbuild": "^0.14.23",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "replace-in-file": "^6.3.2",
    "teckos-client": "^0.2.1",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "ws": "^8.5.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "axios": "^0.26.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "ioredis": "^4.27.9"
  }
}
