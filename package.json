{
  "name": "teckos",
  "version": "0.9.0",
  "description": "uWebsocket.js based server component supporting async callbacks",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "types",
    "src",
    "lib",
    "npm-scripts.mjs",
    "uws"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "clean": "rimraf lib",
    "preinstall": "node npm-scripts.mjs",
    "ts-types": "tsc -p tsconfig.json",
    "es-check": "es-check es2020 lib/index.js --module",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register '__tests__/**/*.ts'",
    "build": "rimraf lib types && node esbuild.config.js && npm run ts-types && npm run es-check",
    "lint": "npx eslint ./src --ext .ts",
    "preversion": "npm run lint && npm run build && npm run es-check && rimraf uws/*",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delude88/teckos.git"
  },
  "keywords": [
    "websocket",
    "uwebsocket",
    "realtime",
    "callbacks"
  ],
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delude88/teckos/issues"
  },
  "homepage": "https://github.com/delude88/teckos#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/ioredis": "^4.28.10",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "es-check": "^6.2.1",
    "esbuild": "^0.14.47",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "teckos-client": "^0.2.5",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "ws": "^8.8.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "axios": "^0.27.2",
    "rimraf": "^3.0.2",
    "uws": "file:./uws"
  },
  "peerDependencies": {
    "ioredis": "^5.0.2"
  }
}
