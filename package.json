{
  "name": "teckos",
  "version": "0.5.4",
  "description": "uWebsocket.js based server component supporting async callbacks",
  "sideEffects": false,
  "type": "commonjs",
  "main": "./lib/index.js",
  "module": "./dist/index.esm.js",
  "types": "./types/index.d.ts",
  "files": [
    "types",
    "src",
    "lib",
    "npm-scripts.mjs"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "postinstall": "node npm-scripts.mjs",
    "ts-types": "tsc -p tsconfig.json",
    "es-check": "es-check es2020 --module dist/index.esm.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register '__tests__/**/*.ts'",
    "build": "rimraf dist lib types && node esbuild.config.cjs && npm run ts-types && npm run es-check",
    "lint": "npx eslint ./src --ext .js,.ts",
    "preversion": "npm run lint && npm run build && npm run es-check && rimraf uws/*.node",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delude88/teckos.git"
  },
  "keywords": [
    "websocket",
    "uwebsocket",
    "realtime",
    "callbacks"
  ],
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delude88/teckos/issues"
  },
  "homepage": "https://github.com/delude88/teckos#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/ioredis": "^4.27.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "es-check": "^6.0.0",
    "esbuild": "^0.13.3",
    "esbuild-node-externals": "^1.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "teckos-client": "^0.2.0",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "ws": "^8.2.2"
  },
  "dependencies": {
    "adm-zip": "^0.5.6",
    "axios": "^0.21.4",
    "ioredis": "^4.27.9",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "debug": "^4.3.2",
    "dotenv": "^10.0.0"
  }
}
