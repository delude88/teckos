{
  "name": "teckos",
  "version": "0.0.9",
  "description": "uWebsocket.js based server component supporting async callbacks",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "ext/**/*"
  ],
  "scripts": {
    "init": "git submodule update --init --recursive",
    "build:ext": "make -C ./ext/uWebSockets && copyfiles -f ./ext/uWebSockets/dist/*.node ./uWebSockets && copyfiles ./ext/uWebSockets/dist/uws.js ./uWebSockets/index.js && copyfiles -f ./ext/uWebSockets/docs/index.d.ts ./uWebSockets",
    "build:lib": "tsc --p tsconfig.json",
    "build": "npm run build:ext && npm run build:lib",
    "lint": "npx eslint ./src --ext .js,.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "postinstall": "npm run build:ext"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delude88/teckos.git"
  },
  "keywords": [
    "websocket",
    "uwebsocket",
    "realtime",
    "callbacks"
  ],
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delude88/teckos/issues"
  },
  "homepage": "https://github.com/delude88/teckos#readme",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/ioredis": "^4.17.8",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "debug": "^4.2.0",
    "ioredis": "^4.19.2",
    "dotenv": "^8.2.0"
  }
}
